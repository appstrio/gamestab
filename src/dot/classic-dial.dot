<div class="dial {{=it.class}}" data-id="{{=it.chromeId}}" data-url="{{=it.url}}">
    <span class="small-badge"></span>
    <a class="inner" href="{{=it.url}}">
        {{? it.icon }}
            <div class="thumbnail-wrapper" style="background-image:url({{=it.icon}})">
                <div class="dial-remove-button">x</div>
            </div>
        {{?? true }}
        <div class="thumbnail-wrapper">
            <div class="dial-remove-button">X</div>
        </div>
        {{? }}
        <div class="title">{{=it.title}}</div>
    </a>
</div>
