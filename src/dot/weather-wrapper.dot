<div class="loading hide">
    <img src="/img/loader.gif"/>
</div>
<div class="weather">

{{~it.days :day:index}}
<div class="day">
    <div class="icon">{{=day.icon}}</div>
    <div class="temp">
        <span class="min">{{=day.temp.min}}&nbsp;</span>
        <span class="max">{{=day.temp.min}}</span>
    </div>
    <div class="title">{{=day.title}}</div>
</div>
{{~}}

</div>
