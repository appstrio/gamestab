{
  "name": "__MSG_appName__",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'",
  "default_locale": "en",
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "img/favicons/16x16.png",
    "48": "img/favicons/48x48.png",
    "128": "img/favicons/128x128.png"
  },
  "optional_permissions": [
    "bookmarks"
  ],
  "permissions": [
    "tabs",
    "history",
    "management",
    "idle",
    "<all_urls>",
    "chrome://favicon/",
    "topSites",
    "alarms",
    "storage",
    "notifications",
    "unlimitedStorage",
    "background",
    "webNavigation",
    "contextMenus",
    "geolocation"
  ],
  "web_accessible_resources": [
    "newtab.html",
    "launcher.html"
  ],
  "version": "1.0.20",
  "manifest_version": 2
}
