    <div class="inner">
        <div style="background-image : url({{=it.screenshot}})"  class="thumbnail screenshot"></div>
        <div class="title">
            <span class="dial-remove-button remove-button">x</span>
            {{=it.title}}
        </div>
    </div>
</div>
